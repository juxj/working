<script>
	function checkgfzx_bak() {
		var txtword = document.getElementById("searchwords").value;
		if(txtword == "" || txtword == "站内搜索")//TextFiled
		{
			alert("请输入检索内容！");
			return false;
		}
		document.getElementById("searchword").value = txtword;
		//设置was form的action
		document.searchform.action = wasUrl + "search";
		var newReg = new RegExp(".gffunds.com.cn/was5");
		if (newReg.test(window.location.href)) document.searchform.target = "_self";
		document.searchform.submit();
	}
function checkgfzx() {
var txtword = document.getElementById("searchwords").value;
if(txtword == "" || txtword == "站内搜索")//TextFiled
{
alert("请输入检索内容！");
return false;
}
document.getElementById("searchword").value = CheckBlank(txtword);
//设置was form的action
document.searchform.action = wasUrl + "search";
var newReg = new RegExp(".gffunds.com.cn/was5");
if(newReg.test(window.location.href))
document.searchform.target = "_self";
document.searchform.submit();
}
function CheckBlank(str){//空格处理函数 
var insertChar = "*";//如果+则为AND关系，*为OR关系
var temStr = str.replace(new RegExp('　', 'g'),' ');//处理所有全角空格
var arrStr = temStr.split(' ');
var reStr = "";
for (var i = 0;i < arrStr.length;i++){
if (arrStr[i]) {
reStr = arrStr[i];
break;
}
}
for (i++;i < arrStr.length;i++){
if (arrStr[i]) {
reStr += insertChar + arrStr[i];
}
}
return reStr;
}
function CheckInsertChar(o, s){
o.value = o.value.replace(new RegExp(s, 'g'),' ');
}
jQuery(document).ready(function(){
CheckInsertChar(document.searchform.searchwords, '\\*');//页面加载完毕之后将检索运算符换回空格
});
function SetHome(obj, vrl) {
		try {
			obj.style.behavior = 'url(#default#homepage)';
			obj.setHomePage(vrl);
		} catch(e) {
			if(window.netscape) {
				try {
					netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
				} catch (e) {
					alert("此操作被浏览器拒绝！\n请在浏览器地址栏输入“about:config”并回车\n然后将[signed.applets.codebase_principal_support]设置为'true'");
				}
				var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
				prefs.setCharPref('browser.startup.homepage', vrl);
			}
		}
	}
