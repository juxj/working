		function loginSSO() {
			var url = window.location.href;
			if(window.location.href.search("was5") > -1) {
				url = "http://www.gffunds.com.cn";
			}

			window.location.href = tUrl + "login.jsp?returnUrl=" + encodeURIComponent(url);
		}

		function askService() {
			var askServiceUrl = "http://live800.gffunds.com.cn/live800/chatClient/chatbox.jsp?companyID=203&configID=78270&enterurl=";
			var thisUrl = window.location.href;
			window.open(askServiceUrl + thisUrl);
		}

		function registerSSO() {
			window.location.href = tUrl + "passport/register_step1.jsp";
		}

		function judgeSSO(url, target, noLoginUrl) {
			if(target && target == "_blank") {
				if(ssoLoginUserName)
					window.open(url);
				else if(noLoginUrl)
					window.open(noLoginUrl);
				else
					window.open(tUrl + "login.jsp?returnUrl=" + url);
				return true;
			}
			if(ssoLoginUserName) {
				window.location.href = url;
			} else {
				if(noLoginUrl)
					window.location.href = noLoginUrl;
				else
					window.location.href = tUrl + "login.jsp?returnUrl=" + url;
			}
		}

		function LoginOut() {
			var t = new date();
			jQuery.getScript(appUrl + "accobook/first/clearSession.jsp?t=" + t, function callback(data) {
				jQuery.getScript(appUrl + "ask/logout.jsp?t=" + t, function callback(data) {
					window.location.href = tUrl + "logout.jsp?returnUrl=" + encodeURIComponent(window.location.href);
				});
			});
		}

		var ssoLoginUserName;
		jQuery(document).ready(function() {
			var ppcURl = "http://www.gffunds.com.cn/dx2/";
			jQuery.ajax({
				url : tUrl + 'passport/myInfo.jsp',
				dataType : 'script',
				async : false,
				data : {},
				success : function(data) {
					var userName = (UserInfo && UserInfo.userName) ? UserInfo.userName : "";
					if(userName) {
						var html = "<a href='http://bbs.gffunds.com.cn/dx2/home.php' title='进入个人中心' style='margin:0px;height:30px;display:inline-block; padding-left:18px;background: url(\"http://www.gffunds.com.cn/images/headerlogin_icon4.png\") no-repeat scroll 0 transparent;'>您好，" + userName + "</a>";
						jQuery("#doLogin").html(html + writeContentIn);
						if (UserInfo.isPassport == false){
							html = "<a href='http://bbs.gffunds.com.cn/dx2/home.php' title='进入个人中心' style='margin:0px;height:30px;display:inline-block; padding-left:18px;background: url(\"http://www.gffunds.com.cn/images/headerlogin_icon4.png\") no-repeat scroll 0 transparent;'>您好，" + userName + "</a>";
							html2 = "&nbsp;&nbsp;<a href='https://sso.gffunds.com.cn/ids/gfsso/passport/idsuser_update_step1.jsp'>升级一站通</a>";	
							//$("#confirm_dialog").html("尊敬的"+userName+"，广发基金网站已升级为一站通，简单升级即可全站通行。只需填写简单资料，即可完成升级！");
							//$("#confirm_dialog").dialog("open");
