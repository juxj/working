function follow(n,funcode){//我要关注
	if(UserInfo != null){
                           $.ajax({url:AccobookURL + "AttentionFund.jsp?fundId=" + funcode+ "&userName=" + encodeURI(encodeURI(UserInfo.userName)) + "&now=" + new Date().getTime(), type:"get", success:function(data){
			var html = data.split("\n");
			var Check = html[html.length - 1];
                       if(Check  ==0){
                                    $("#favourite"+n+"").attr("class","gray");
                                   $("#favourite"+n+"").html("<a href='javascript:void(0)'>已关注</a>");      
                        }
                        else{
                                   $("#favourite"+n+"").html("<a onclick='follow("+n+","+funcode+");'>关注</a>");
                                   $("#favourite"+n+"").attr("class","orange");
                             }
 alertPlus(Check,["-4","-3","-2","-1","0","1","2"],["关注基金不存在","传入参数用户名在系统中不存在","参数用户名为空","参数基金代码不为空","关注基金成功","关注基金失败","已经关注此基金"]);
		}
		});
	}
         else{
		alert("请登录！");
		loginSSO();
	}
}
function alertPlus(Check,CheckArr,AlertArr){//Check == CheckArr[i];alert(AlertArr[i]);
	for (i=0;i < CheckArr.length;i++) if (Check == CheckArr[i]) {alert(AlertArr[i]);break;};
}


function showfollow(n,funcode){//检查关注      //alert(UserInfo.userName);
	if (UserInfo != null){
		$.ajax({url:AccobookURL + "isAttentionFund.jsp?fundId=" + funcode+ "&userName=" + encodeURI(encodeURI(UserInfo.userName)), type:"get", success:function(data){
			var html = data.split("\n");
                          var Check = html[html.length - 1];
                               if(Check==1){
                                   $("#favourite"+n+"").attr("class","gray");
                                   $("#favourite"+n+"").html('<a href="javascript:void(0)">已关注</a>');      
                            }else{
                                   $("#favourite"+n+"").attr("class","orange");
                                   $("#favourite"+n+"").html("<a onclick='follow("+n+","+funcode+");'>关注</a>");  
                            }
	       }
		});
	} else{
                                   $("#favourite"+n+"").attr("class","orange");
                                   $("#favourite"+n+"").html("<a  onclick='javascript:alert(\'请登录\');loginSSO();'>我要关注</a>");
	}

     }


function renderProfitAndLossColor(n,extra){
            var s = n;
            if (n > 0) s = "<font color=red>" + n + extra + "</font>";
            else if (n < 0)
            s = "<font color=green>" + n + extra + "</font>";
            else s = n + extra;
            return s;
}
//表格排序脚本 By TRS_herijing 
function initTable($table) {//初始化需要排序的表格（表格jQuery对象）
	$table.find('tr').eq(0).find('td').each(function(n) {
		$(this).unbind("click").click(function() {
			runTable($table, n)
		}).css("cursor", "pointer");
	});
}

function runTable($table, TDnum, Order) {//执行排序（表格jQuery对象，第几个单元格，顺序desc，asc）
	var strAsc = "↑";
	var strDesc = "↓";
	if(Order) {
		if((Order != "desc" && Order != "asc")) {
			Order = "desc";
		}
	} else {
		Order = "desc";
	}
	if($table && TDnum) {
		$table.find("tr").filter(":gt(0)").each(function(n) {
			var Tnum = GetTDFloat($(this), TDnum);
			//alert(Tnum);
			var temHtml = "";
			var $T = $(this);
			$table.find("tr").filter(":gt(" + n + ")").each(function(n1) {
				var Snum = GetTDFloat($(this), TDnum);
				var Do = Order == "desc" ? (Snum > Tnum) : (Tnum > Snum);
				if(Do) {
					Tnum = Snum;
					temHtml = $T.html();
					$T.html($(this).html());
					$(this).html(temHtml);
				}
			});
		});
		initTable($table);
		$table.find('tr').eq(0).find('td').each(function(n) {
			$(this).html($(this).html().replace(strAsc, '').replace(strDesc, ''));
			if(n == TDnum) {
				$(this).unbind("click").click(function() {
					runTable($table, n, Order == "desc" ? "asc" : "desc");
				}).append(Order == "desc" ? strDesc : strAsc);
			}
		});
	} else
		return false;
}

function GetTDFloat($tr, nTD) {//获取单元格数据（tr jQuery对象，第几个单元格）
	return $tr.find('td').eq(nTD).text() != "——" ? parseFloat($tr.find('td').eq(nTD).text()) : -9999;
}
//表格排序脚本 结束

</script>

