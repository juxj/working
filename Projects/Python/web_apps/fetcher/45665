 <script type="text/javascript">
  function doStatus(){
		try{
		    var st = document.getElementById("sell_s").innerText;
				var et = document.getElementById("sell_e").innerText;
				if(st &amp;&amp; et &amp;&amp; ""!=st &amp;&amp; ""!=et){
					var s = new Date(st+" 00:00:00");
					var c = new Date();
					var e = new Date(et+" 00:00:00");
					if(c&lt;s){
						  document.getElementById("sell_c").innerText = "未开始销售";
					}else if(c&gt;s &amp;&amp; c&lt;e){
						  document.getElementById("sell_c").innerText = "正在销售中";
					}else if(c&gt;e){
						  document.getElementById("sell_c").innerText = "已结束销售";
					}else{
						  document.getElementById("sell_c").innerText = "";
					}
				}
		}catch(err){
		   document.getElementById("sell_c").innerText = "";
		}
	} 
	doStatus();
 </script>
