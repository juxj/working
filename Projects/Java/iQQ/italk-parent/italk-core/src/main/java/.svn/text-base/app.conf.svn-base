#app info
app.name=iQQ
app.version=0.1
#app.mode could be DEV or PROD
app.mode=dev
#log
app.log.path=/log4j.properties
#keep this as low as possible
threads.poolSize=4

#设置窗口透明度 0.0 -- 1.0
windowOpacity = 1
#是否启用皮肤文件 true & false
isEnableSkin = true
#是否启用皮肤水印
isEnableWatermark = true

#资源目录
resource_dir_name = resource
#图片目录
images_dir_name = images
#皮肤水印目录
skin_watermark_dir = skinWatermark
#皮肤水印文件
skin_watermark_file = watermark.png
#皮肤
skin_class = italk.ui.skin.MyBusinessBlackSteelSkin
#国际化资源目录
i18n_dir = i18n
#国际化资源文件
i18n_bundle_name = appBundle
