<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
	
<struts>
	<package name="finance" namespace="/finance" extends="base">
	
		<action name="fund" class="com.zj198.action.finance.FundAction">
			<result name="home">/public/finance/fundHome.jsp</result>
			<result name="history">/public/finance/fundDetails.jsp</result>
		</action>
		<action name="bankProduct" class="com.zj198.action.finance.BankProductAction">
			<result name="home">/public/finance/bankFinanceHome.jsp</result>
			<result name="details">/public/finance/financeProductDetails.jsp</result>
		</action>
		
		<action name="financeProduct" class="com.zj198.action.finance.FinanceProductAction">
			<result name="success">/public/finance/financeServices.jsp</result>
			<result name="catering">/public/finance/cateringFund.jsp</result>
		</action>
		
	</package>
</struts>