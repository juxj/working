<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="cfs" namespace="/cfs" extends="base">
	
		<action name="showCompletedFiscalYears" class="com.zj198.action.cfs.CfsMetricAction" method="showCompletedFiscalYears">
    	   <result name="completedReports">/WEB-INF/web/user/cfs/completedReports.jsp</result>
    	</action>
    	
    	<action name="showFinacingMetricReport" class="com.zj198.action.cfs.CfsMetricAction" method="showFinacingMetricReport">
    	   <result name="finacingMetricReport">/WEB-INF/web/user/cfs/finacingMetricReport.jsp</result>
    	</action>
    	
    	<action name="showComparedReport" class="com.zj198.action.cfs.CfsMetricAction" method="showComparedReport">
    	   <result name="commonSizeRpt">/WEB-INF/web/user/cfs/commonSizeRpt.jsp</result>
    	</action>
    	
    	<action name="showInitializePage" class="com.zj198.action.cfs.CfsReportAction" method="showInitializePage">
    	   <result name="initialize">/WEB-INF/web/user/cfs/initialize.jsp</result>
    	   <result name="showRptsInProcess" type="redirect">/cfs/showRptsInProcess.act</result>
    	</action>
    	
    	<action name="showReportForm" class="com.zj198.action.cfs.CfsReportAction" method="showReportForm">
    	   <result name="report">/WEB-INF/web/user/cfs/report.jsp</result>
    	</action>
    	
    	<action name="saveReportData" class="com.zj198.action.cfs.CfsReportAction" method="saveReportData">
    	   <result name="showRptsInProcess" type="redirect">/cfs/showRptsInProcess.act</result>
    	</action>
    	
    	<action name="showRptsInProcess" class="com.zj198.action.cfs.CfsReportAction" method="showRptsInProcess">
    	   <result name="reports_in_process">/WEB-INF/web/user/cfs/reportsInProcess.jsp</result>
    	   <result name="showCompletedFiscalYears" type="redirect">/cfs/showCompletedFiscalYears.act</result>
    	</action>
    	
    	<action name="getReportDataByJson" class="com.zj198.action.cfs.CfsReportAction" method="getReportDataByJson">
    	   <result name="json">/WEB-INF/web/user/cfs/json.jsp</result>
    	</action>
    	
    	<action name="saveUsrRptConfig" class="com.zj198.action.cfs.CfsReportAction" method="saveUsrRptConfig">
    	  <result name="showRptsInProcess" type="redirect">/cfs/showRptsInProcess.act</result>
    	</action>
    	
    	
    	
	</package>
</struts>