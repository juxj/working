<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
	
<struts>
	<package name="loan" namespace="/loan" extends="base">
		<action name="financeProduct" class="com.zj198.action.loan.FinanceProductAction">
			<result name="financeSearch">/WEB-INF/web/user/loan/finance_product_search.jsp</result>
			<result name="financeSearchDetail">/WEB-INF/web/user/loan/finance_product_searchDetail.jsp</result>
			<result name="purposeTypeList">/public/common/purposeTypeList.jsp</result>
			<result name="industryList">/public/common/industryList.jsp</result>
			<result name="custom_view_finance">/WEB-INF/web/user/loan/custom_view_finance.jsp</result>
			<result name="recommendProduct">/public/loan/recommendProduct.jsp</result>
		</action>
		<action name="LoanService" class="com.zj198.action.loan.LoanServiceAction" >
			<result name="financing">/public/loan/financing_Service.jsp</result>
		</action>
		<action name="LoanRequest" class="com.zj198.action.loan.LoanRequestAction" >
			<result name="busiFirst">/public/loan/busiloan_request_first.jsp</result>
			<result name="loanSecond">/public/loan/busiloan_request_second.jsp</result>
			<result name="loanThird">/public/loan/busiloan_request_third.jsp</result>
			<result name="financing">/public/loan/financing_Service.jsp</result>
			<!-- 未审核的用户填写profile信息后提交action  不考虑激活 -->
			<result name="profileSuccess" type="redirect">/user/UserAction.act</result>
			<!-- begin个人综合消费快速申请(add by zeroleavebaoyang@gmail.com) -->
			<result name="percostFirst">/public/loan/percost_request_first.jsp</result>
			<!-- end个人综合消费快速申请(add by zeroleavebaoyang@gmail.com) -->
		
			<!-- begin个人经营贷款快速申请(add by zeroleavebaoyang@gmail.com) -->
			<result name="perrunFirst">/public/loan/perrun_request_first.jsp</result>
			<!-- end个人经营贷款快速申请(add by zeroleavebaoyang@gmail.com) -->
		
			<!-- begin个人购房贷款快速申请(add by zeroleavebaoyang@gmail.com) -->
			<result name="perhouseFirst">/public/loan/perhouse_request_first.jsp</result>
			<!-- end个人购房贷款快速申请(add by zeroleavebaoyang@gmail.com) -->
			
			<result name="index" type="redirect">/Account.act</result>
			<!-- end融资需求管理(add by zeroleavebaoyang@gmail.com) -->
			<result name="busi_request_view">/WEB-INF/web/user/loan/busiloan_request_view.jsp</result>
			<result name="purposes">/public/common/purposeTypeList.jsp</result>
		</action>
	</package>
	<package name="user_loan" namespace="/user/loan" extends="base">
		<default-interceptor-ref name="userInterceptor"/>
		<action name="LoanRequest" class="com.zj198.action.loan.UserLoanRequestAction" >
			<!-- begin融资需求管理(add by zeroleavebaoyang@gmail.com) -->
			<result name="loanManage">/WEB-INF/web/user/loan/loanManage.jsp</result>
			<result name="loanManageDetail">/WEB-INF/web/user/loan/loanManage_detail.jsp</result>
			<result name="index" type="redirect">/Account.act</result>
			<!-- end融资需求管理(add by zeroleavebaoyang@gmail.com) -->
			<result name="busi_request_view">/WEB-INF/web/user/loan/busiloan_request_view.jsp</result>
			<result name="purposes">/public/common/purposeTypeList.jsp</result>
			<result name="loanRequestHome">/WEB-INF/web/user/common/loanTopThree.jsp</result>
		</action>
		<action name="financeApply" class="com.zj198.action.loan.FinanceApplyAction">
			<result name="busi_apply_first">/WEB-INF/web/user/loan/busifinance_apply_first.jsp</result>
			<result name="apply_second">/WEB-INF/web/user/loan/finance_apply_second.jsp</result>
			<result name="view_busi_apply">/WEB-INF/web/user/loan/busifinance_apply_view.jsp</result>
			<result name="view_perrun_apply">/WEB-INF/web/user/loan/perrunfinance_apply_view.jsp</result>
			<result name="view_percost_apply">/WEB-INF/web/user/loan/percostfinance_apply_view.jsp</result>
			<result name="view_perhouse_apply">/WEB-INF/web/user/loan/perhousefinance_apply_view.jsp</result>
			<result name="industryList">/public/common/industryList.jsp</result>
			<result name="user_apply_mana">/WEB-INF/web/user/loan/user_apply_mana.jsp</result>
			<result name="apply_mana_org">/WEB-INF/web/user/loan/apply_mana_org.jsp</result>
			<result name="apply_upload_attach">/WEB-INF/web/user/loan/apply_upload_attach.jsp</result>
			<result name="percostApplyFirst">/WEB-INF/web/user/loan/percostfinance_apply_first.jsp</result>
			<result name="perrunApplyFirst">/WEB-INF/web/user/loan/perrunfinance_apply_first.jsp</result>
			<result name="perhouseApplyFirst">/WEB-INF/web/user/loan/perhousefinance_apply_first.jsp</result>
			<result name="industryList">/public/common/industryList.jsp</result>
			<result name="applyFailure">/WEB-INF/web/user/loan/apply_failure.jsp</result>
			<result name="findApplyHome">/WEB-INF/web/user/common/financeApplyList.jsp</result>
			<result name="complementUserInfo" type="chain">
	            <param name="actionName">Profile</param>
	            <param name="namespace">/user</param>
	            <param name="method">edit</param>
	          </result>
	        <result name="companyInfoEdit">/WEB-INF/web/user/loan/busifinance_apply_company.jsp</result>
	        <result name="personInfo">/WEB-INF/web/user/loan/perrun_apply_person.jsp</result>
		</action>
		<action name="financeProduct" class="com.zj198.action.loan.UserFinanceProductAction">
			<result name="new_finanace_page">/WEB-INF/web/user/loan/finance_product_new.jsp</result>
			<result name="view_finance">/WEB-INF/web/user/loan/finance_product_view.jsp</result>
			<result name="recommend">/public/common/recommendProList.jsp</result>
			<result name="update_finance_page">/WEB-INF/web/user/loan/finance_product_update.jsp</result>
			<result name="previewExtendsProperty">/WEB-INF/web/user/loan/previewExtendsProperty.jsp</result>
			<result name="financeMana">/WEB-INF/web/user/loan/finance_product_mana.jsp</result>
			<result name="update_success">/WEB-INF/web/user/loan/update_finance_back.jsp</result>
		</action>	
		<!-- 
		<action name="financeManager" class="com.zj198.action.loan.FinanceProductManaAction">
			<result name="view_finance">/WEB-INF/web/user/loan/finance_product_view.jsp</result>
		</action>
			<result name="input">/WEB-INF/web/user/loan/apply_upload_attach.jsp</result>
			<result name="input">/user/loan/financeAttach!attachMana.act?viewAttachType=${viewAttachType}&amp;applyId=${applyId}&amp;msg=error</result>
		 -->
		<action name="financeAttach" class="com.zj198.action.loan.FinanceAttachAction">
            <interceptor-ref name ="defaultStack" />
			<interceptor-ref name ="fileUpload" > 
				<param name="allowedTypes">image/bmp,image/x-png,image/gif,image/jpg,image/jpeg,application/excel,application/vnd.ms-excel,application/msword,application/pdf</param>
				<param name="maximumSize">3025956</param>
            </interceptor-ref > 
			<result name="apply_upload_attach">/WEB-INF/web/user/loan/apply_upload_attach.jsp</result>
			<result name="downloan_attach">/WEB-INF/web/user/loan/download_attach.jsp</result>
			<result name="input">/WEB-INF/web/user/loan/upload_result.jsp?msg=error</result>
			<result name="success">/WEB-INF/web/user/loan/upload_result.jsp</result>
		</action>
		
		<!-- 贷后管理 -->
		<action name="loanAfter" class="com.zj198.action.loan.LoanAfterAction">
			<result name="loanAfterDetail">/WEB-INF/web/user/loan/detail_app_loan.jsp</result>
			<result name="loanAfterAddUI">/WEB-INF/web/user/loan/input_app_loan.jsp</result>
			<result name="loanAfterAdd" type="redirect">/user/loan/loanAfter!loanAfterList.act</result>
			<result name="loanAfterList">/WEB-INF/web/user/loan/list_app_loan.jsp</result>
			<result name="userAppLoan">/WEB-INF/web/user/loan/user_app_loan.jsp</result>
			<result name="loanAppAttach">/WEB-INF/web/user/loan/loan_app_attach.jsp</result>
		</action>
		<action name="appLoanAttach" class="com.zj198.action.loan.AppLoanAttachAction">
            <interceptor-ref name ="defaultStack" />
			<interceptor-ref name ="fileUpload" > 
				<param name="allowedTypes">image/bmp,image/x-png,image/gif,image/jpg,image/jpeg,application/excel,application/vnd.ms-excel,application/msword,application/pdf</param>
				<param name="maximumSize">3025956</param>
            </interceptor-ref > 
			<result name="loanAppAttach">/WEB-INF/web/user/loan/loan_app_attach.jsp</result>
			<result name="downloan_attach">/WEB-INF/web/user/loan/download_attach_appLoan.jsp</result>
			<result name="input">/WEB-INF/web/user/loan/upload_result.jsp?msg=error</result>
			<result name="success">/WEB-INF/web/user/loan/upload_result.jsp</result>
		</action>
		
		<!-- 产品推荐 -->
		<action name="recommendPro" class="com.zj198.action.loan.OrdLrRecommendAction">
			<result name="lookRecPro">/WEB-INF/web/user/loan/recommend_product_list.jsp</result>
		</action>
		
		<action name="downloadAttach" class="com.zj198.action.loan.FileDownloadAction">
			 <result name="success" type="stream">
			 	<param name="contentDisposition">attachment;filename="${fileName}"</param>
			 	<param name="contentType">application/octet-stream;charset=UTF-8</param>
			 	<param name="inputName">attachStream</param>
			 	<param name="bufferSize">4096</param>  
			 </result>
			 <result name="downAgree" type="stream">
			 	<param name="contentDisposition">attachment;filename="${fileName}"</param>
			 	<param name="contentType">application/octet-stream;charset=UTF-8</param>
			 	<param name="inputName">downAgreeStream</param>
			 	<param name="bufferSize">4096</param>  
			 </result>
			 <result name="template" type="stream">
			 	<param name="contentDisposition">attachment;filename="${fileName}"</param>
			 	<param name="contentType">application/octet-stream;charset=UTF-8</param>
			 	<param name="inputName">agreeTemplateStream</param>
			 	<param name="bufferSize">4096</param>  
			 </result>
		</action>
	</package>

</struts>