<%@ page language="java" contentType="text/html; charset=UTF-8"%>
<%@ taglib uri="/struts-tags" prefix="s" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=EmulateIE7" />
<title>完善信息（个人）用户中心_中国资金网</title>
<link rel="stylesheet" href="/css/public.css" type="text/css" media="screen, projection" />
<link rel="stylesheet" href="/css/member.css" type="text/css" media="screen, projection" />
<link rel="stylesheet" href="/css/member1.css" type="text/css" media="screen, projection" />
<script type="text/javascript" src="/script/jquery-1.7.2.min.js" > </script>
<script type="text/javascript" src="/script/profile.js" > </script>
</head>
<body>
<!--头部-->
<div class="header user_header">
<jsp:include page="/WEB-INF/web/user/head.jsp"></jsp:include>
</div>
<div class="hr_10"> &nbsp; </div>
<!--主体部分-->
<div class="container_950 box_4">
	<div class="P_title">详细信息</div>
	<div class="line">
	<s:if test="usrUser == null || #session._user.id == usrUser.id">
		<img src="/images/icon01.jpg" alt="" class="P_connect_img"/>当前账户状态:
		<s:if test="#session._user.auditstatus==0"><span class="no_ok">未审核</span></s:if>
		<s:elseif test="#session._user.auditstatus==1"><span class="no_ok">待审核</span></s:elseif>
		<s:elseif test="#session._user.auditstatus==2"><span class="no_ok">已审核</span></s:elseif>
	</div>
	</s:if>
</div>
<div class="hr_10"> &nbsp; </div>
<!--tab-->
<div class="box_4 center" style="width:950px;">
	<div class="news_menu" style="height: 50px;">
        <dl style="margin-left: 30px;">
          <dd><h6>个人基本信息</h6></dd>
          <dd>
          	<s:if test="usrPerson.completerate==null || usrPerson.completerate<60">
				<div class="graph_red"><span style="width:${usrPerson.completerate+0}%"></span></div>
			</s:if><s:elseif test="60<=usrPerson.completerate && usrPerson.completerate<100">
				<div class="graph_orange"><span  style="width:${usrPerson.completerate}%"></span></div>
			</s:elseif><s:else>
				<div class="graph_green"><span  style="width:${usrPerson.completerate}%"></span></div>
			</s:else>
          </dd>
        </dl>
      </div>
    <form class="box_form">
      <dl>
      <s:if test="msg!=null">
	    <dd>
		  <div class="notification png_bg">
	        <div>${msg }</div>
	      </div>
	    </dd>
	    </s:if>
        <dd>
          <h6>姓名：</h6>
          <div class="text">${session._user.realname}</div>
        </dd>
        <dd>
          <h6>身份证号码：</h6>
          <div class="text">${usrPerson.cid}</div>
        </dd>
        <dd>
          <h6>所在地：</h6>
          <div class="text">${profileMap['address'] }</div>
        </dd>
        <dd>
          <h6>详细地址：</h6>
          <div class="text">${usrPerson.address }</div>
        </dd>
        <dd>
          <h6>邮政编码：</h6>
          <div class="text">${usrPerson.postcode }</div>
        </dd>
        <dd>
          <h6>职业：</h6>
          <div class="text">${profileMap['psersoncareer'] }</div>
        </dd>
        <dd>
          <h6>固定电话：</h6>
          <div class="text">${usrPerson.telephone }</div>
        </dd>
        <dd>
          <h6>联系方式：</h6>
         <div class="text">${profileMap['imtype1'] }&nbsp;&nbsp; ${usrPerson.im1 }</div>
        </dd>
        <dd>
          <h6>&nbsp;</h6>
          <div class="text">${profileMap['imtype2'] }&nbsp;&nbsp;${usrPerson.im2 }</div>
        </dd>
        <dd>
          <h6>&nbsp;</h6>
         <div class="text">${profileMap['imtype3'] }&nbsp;&nbsp;${usrPerson.im3 }</div>
        </dd>
      </dl>
      <div class="hr_10"> &nbsp; </div>
      <hr width="90%" />
      <div class="hr_10"> &nbsp; </div>
      <div class="hr_10"> &nbsp; </div>
      <s:if test="usrUser == null || #session._user.id == usrUser.id">
      <div class="center" style="width:200px;">
        <input type="button" class="but_gray" onclick="javascript:window.location.href='/user/Profile!edit.act';" style="width:100px;" value="编辑信息" />
      </div>
      </s:if>
      <div class="hr_10"> &nbsp; </div>
      <div class="hr_10"> &nbsp; </div>
    </form>
    <div class="hr_10"> &nbsp; </div>
    <div class="hr_10"> &nbsp; </div>
    <div class="hr_10"> &nbsp; </div>
</div>
<div class="hr_10"> &nbsp; </div>
<!--尾部-->
<jsp:include page="/public/bottom.jsp"></jsp:include>
</body>
</html>