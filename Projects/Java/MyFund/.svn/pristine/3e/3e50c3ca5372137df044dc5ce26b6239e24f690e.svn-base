<%@ page language="java" contentType="text/html; charset=UTF-8"%>
<%@ taglib uri="/struts-tags" prefix="s"%>
<%@taglib prefix="common" uri="/Common"%>
<div class="user_login01">
	<div class="clear">&nbsp;</div>
	<div class="M_box">
	<input type="hidden" id="requestId" name="requestId" value="<s:property value="requestId"/>">	
		<table border="0" cellspacing="0" cellpadding="0" style="width:100%">
			<tr class="BankProductHeader">
				<th></th>
				<th>产品编号</th>
				<th>产品名称</th>
				<th>类别</th>
				<th>年利率</th>
				<th>发布时间</th>
				<th>额度范围</th>
				<th>期限</th>
			</tr>
			<s:iterator value="recommendProducts">
				<tr class="gold_connect">
					<td><input type="checkbox" name="checkId" <s:if test="isRecommend ==1 ">checked="checked"</s:if> value="${id }"/></td>
					<td>${id }</td>
					<td>${financeName }</td>
					<td><common:print valueId="${financeType }"/></td>
					<td>${littleInterest}~${bigInterest }%</td>
					<td><s:date name="createdt" /></td>
					<td>${financeLittleamount }~${financeBigamount }万元</td>
					<td>${financeLittledt }~${financeMostdt }月</td>
				</tr>
			</s:iterator>
			<tr  class="gold_connect" >
				<td colspan="8" align="c"><input type="button" onclick="SelectRecom()" value="选择" class="but_gray" />&nbsp;<input type="button" name="cancel" value="取消" onclick="$('#pro_recommend').dialog('close');" class="but_gray"/> </td>
			</tr>
		</table>
	</div>
</div>