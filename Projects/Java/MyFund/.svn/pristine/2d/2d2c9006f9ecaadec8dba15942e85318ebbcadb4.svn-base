<%@ page language="java" contentType="text/html; charset=UTF-8"%>
<%@ taglib prefix="s" uri="/struts-tags" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=EmulateIE7" />
<title>企业用户信息中心_中国资金网</title>
<link rel="stylesheet" href="/css/public.css" type="text/css" media="screen, projection" />
<link rel="stylesheet" href="/css/member.css" type="text/css" media="screen, projection" />
<link rel="stylesheet" href="/css/member1.css" type="text/css" media="screen, projection" />
<script type="text/javascript" src="/script/jquery-1.7.2.min.js" > </script>
<script language="javascript">
$(document).ready(function(){
	$(".input-text").attr({style:"border:0;background:transparent;",readonly:"readonly"});
});
</script>
</head>
<body>
<!--头部-->
<div class="header user_header">
	<jsp:include page="/WEB-INF/web/user/head.jsp"></jsp:include>
</div>
<div class="hr_10"> &nbsp; </div>
<!--主体部分-->
<div class="container_950 box_4">
	<div class="P_title">详细信息</div>
	<div class="line">
	<s:if test="usrUser == null || #session._user.id == usrUser.id">
		<img src="/images/icon01.jpg" alt="" class="P_connect_img"/>当前账户状态:
		<s:if test="#session._user.auditstatus==0"><span class="no_ok">未审核</span></s:if>
		<s:elseif test="#session._user.auditstatus==1"><span class="no_ok">待审核</span></s:elseif>
		<s:elseif test="#session._user.auditstatus==2"><span class="no_ok">已审核</span></s:elseif>
	</div>
	</s:if>
</div>
<div class="hr_10"> &nbsp; </div>
<!--tab-->
<div class="box_4 center" style="width:950px;">
      <div class="news_menu" style="height: 50px;">
        <dl style="margin-left: 30px;">
          <dd><h6>企业基本信息</h6></dd>
          <dd>
       		 <s:if test="usrCompany.completeratebase==null || usrCompany.completeratebase<60">
				<div class="graph_red"><span style="width:${usrCompany.completeratebase+0}%;"></span></div>
			</s:if><s:elseif test="60<=usrCompany.completeratebase && usrCompany.completeratebase<100">
				<div class="graph_orange"><span  style="width:${usrCompany.completeratebase}%;"></span></div>
			</s:elseif><s:else>
				<div class="graph_green"><span  style="width:${usrCompany.completeratebase}%;"></span></div>
			</s:else>
          </dd>
        </dl>
      </div>
	<form class="box_form">
         <dl>
         <s:if test="msg!=null">
	    <dd>
		  <div class="notification png_bg">
	        <div>${msg }</div>
	      </div>
	    </dd>
	    </s:if>
	      <dd>
	        <h6>企业名称：</h6>
	        <div class="text">${usrCompany.companyname }</div>
	       <h6>营业执照号码：</h6>
	       <div class="text">${usrCompany.licensecode }</div>
	     </dd>
	     <dd>
	       <h6>组织机构代码证号码：</h6>
	       <div class="text">${usrCompany.organizationcode }</div>
	       <h6>税务登记证号码：</h6>
	       <div class="text">${usrCompany.faxcode }</div>
	     </dd>
	     <dd>
	       <h6>开户许可证号码：</h6>
	       <div class="text ">${usrCompany.bankpermitcode }</div>
	       <h6>贷款卡号：</h6>
	       <div class="text">${usrCompany.loancard }</div>
	     </dd>
	     <dd>
	       <h6>法定代表人：</h6>
	       <div class="text">${usrCompany.legalperson }</div>
	     </dd>
	     <dd>
	       <h6>注册地址：</h6>
	       <div class="text">${profileMap['regaddress'] }</div>
	     </dd>
	     <dd>
	       <h6>详细地址：</h6>
	       <div class="text">${usrCompany.regaddress }</div>
	     </dd>
	     <dd>
	       <h6>邮编：</h6>
	       <div class="text">${usrCompany.regpostcode }</div>
	     </dd>
	     <dd>
	       <h6>注册日期：</h6>
	       <div class="text"><s:date name="usrCompany.registerdate"/></div>
	       <h6>注册资本：</h6>
	       <s:if test="usrCompany.registeredcapital!=null">
	        <div class="text"><s:text name="format.float"><s:param value="usrCompany.registeredcapital"></s:param></s:text>&nbsp;万元</div>
	       </s:if>
	     </dd>
	     <dd>
	       <h6>企业类型：</h6>
	       <div class="text">${profileMap['companyType'] }</div>
	       <h6>员工人数：</h6>
	       <div class="text">${profileMap['companyTmployee'] }</div>
	     </dd>
	     <dd>
	       <h6>所属行业：</h6>
	       <div class="text">${profileMap['industry'] }</div>
	     </dd>
	     <dd>
	       <h6>经营范围：</h6>
	       <div class="text">${usrCompany.bizscope }</div>
	     </dd>
	     <dd>
	       <h6>企业经营地址：</h6>
	       <div class="text">${profileMap['bizaddress'] } </div>
	     </dd>
	     <dd>
	     <h6>企业经营详细地址：</h6>
	     	<div class="text">${usrCompany.bizaddress }</div>
	     </dd>
	     <dd>
	       <h6>邮编：</h6>
	       <div class="text">${usrCompany.bizpostcode }</div>
	     </dd>
	     <dd>
	       <h6>联系人姓名：</h6>
	       <div class="text">${usrCompany.linkname} 
	       <s:if test="usrCompany.linkgender==1">先生</s:if><s:elseif test="usrCompany.linkgender==0">女士</s:elseif></div>
	     </dd>
	     <dd>
	       <h6>联系人邮箱：</h6>
	       <div class="text">${usrCompany.linkemail }</div>
	       <h6>联系人电话：</h6>
	       <div class="text">${usrCompany.linktelephone }</div>
	     </dd>
	     <dd>
	       <h6>所属部门：</h6>
	       <div class="text">${usrCompany.department }</div>
	       <h6>职位：</h6>
	       <div class="text">${usrCompany.position }</div>
	     </dd>
       </dl>
        <dl>
		  <dt>企业法人信息</dt>
	     <dd>
	       <h6>学历：</h6>
	       <div class="text">${profileMap['eduAtion'] }</div>
	     </dd>
	     <dd>
	       <h6>婚姻状况：</h6>
	       <div class="text">${profileMap['marry'] }</div>
	       <h6>从事所属行业年限：</h6>
	       <div class="text">${profileMap['workYears'] }</div>
	     </dd>
	     <dd>
	       <h6>身份证号码：</h6>
	       <div class="text">${usrCompany.lpcid }</div>
	     </dd>
	     <dd>
	       <h6>手机：</h6>
	       <div class="text">${usrCompany.lpmobile}</div>
	       <h6>家庭电话：</h6>
	       <div class="text">${usrCompany.lphometel }</div>
	     </dd>
	     <dd>
	       <h6>常住地址：</h6>
	       <div class="text">${profileMap['lpliveaddress'] }</div>
	     </dd>
	     <dd>
	       <h6>常住详细地址：</h6>
	      <div class="text"> ${usrCompany.lpliveaddress }</div>
	     </dd>
	     <dd>
	       <h6>邮编：</h6>
	       <div class="text">${usrCompany.lplivepostcode }</div>
	     </dd>
	     <dd>
	       <h6>户籍所在地：</h6>
	       <div class="text">${profileMap['lphkaddress'] }</div>
	     </dd>
	     <dd>
	       <h6>户籍所在地详细地址：</h6>
	       <div class="text">${usrCompany.lphkaddress }</div>
	     </dd>
	     <dd>
	       <h6>邮编：</h6>
	       <div class="text">${usrCompany.lphkpostcode }</div>
	     </dd>
       </dl>
       <hr width="96%" />
       <div class="hr_10"> &nbsp; </div>
       <div class="hr_10"> &nbsp; </div>
       <div class="hr_10"> &nbsp; </div>
       <s:if test="usrUser == null || #session._user.id == usrUser.id">
	       <div class="center" style="width:200px;">
	         <input type="button" onclick="javascript:window.location.href='/user/Profile!edit.act';" class="but_gray" style="width:200px;" value="编辑信息" />
	        </div>
	    </s:if>
        <div class="hr_10"> &nbsp; </div>
        <div class="hr_10"> &nbsp; </div>
        </form>
</div>
<div class="hr_10"> &nbsp; </div>
<!--尾部-->
<jsp:include page="/public/bottom.jsp"></jsp:include>
</body>
</html>